<?xml version="1.0" encoding="UTF-8"?>
<model name="barcode:barcodeModel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

    <description>Barcode Model</description>
    <version>1.0</version>

    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
        <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>
    </imports>

    <namespaces>
        <namespace uri="http://www.beone.pl/model/barcode/1.0" prefix="barcode"/>
    </namespaces>

    <aspects>
        <aspect name="barcode:metadata">
            <title>Barcode</title>
            <properties>
                <property name="barcode:barcode">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                </property>
                <property name="barcode:startPageOfOriginalFile">
                    <type>d:int</type>
                    <mandatory>true</mandatory>
                </property>
                <property name="barcode:endPageOfOriginalFile">
                    <type>d:int</type>
                    <mandatory>true</mandatory>
                </property>
            </properties>
        </aspect>
    </aspects>

</model>